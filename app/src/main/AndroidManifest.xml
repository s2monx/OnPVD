<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onpvd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Sach_acti"></activity>
        <activity android:name=".Tacgia_acti" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".MyContentProvider"
            android:authorities="com.example.onpvd.MyContentProvider"
            android:enabled="true"
            android:exported="true"
            android:writePermission="com.example.onpvd.WRITE_DATABASE"
            android:readPermission="com.example.onpvd.READ_DATABASE"/>
    </application>

</manifest>